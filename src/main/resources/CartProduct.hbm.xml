<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-5.3.5.dtd"> 

<hibernate-mapping>
    <class name = "com.ideas2it.ecommerce.model.CartProduct" table = "CART_PRODUCT">
      
        <meta attribute = "class-description">
            This class contains the Cart Product details. 
        </meta>
      
        <id name = "id" type = "int" column = "ID">
            <generator class="increment"/>
        </id>
      
        <property name = "quantity" column = "QUANTITY" type = "int"/>
        <property name = "price" column = "PRICE" type = "float"/>
        
        <many-to-one name = "customer" column = "CUSTOMER" 
            class = "com.ideas2it.ecommerce.model.Customer"/>
        
        <many-to-one name = "warehouseProduct" column = "WAREHOUSE_PRODUCT" 
            class = "com.ideas2it.ecommerce.model.WarehouseProduct"/>
    </class>
</hibernate-mapping>