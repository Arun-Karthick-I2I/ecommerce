<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.3//EN" "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
     
<hibernate-mapping>
	<class name="com.ideas2it.ecommerce.model.WarehouseProduct" table="WAREHOUSE_PRODUCT">
		<id name="id" column="ID" type="integer">
			<generator class="increment"></generator>
		</id>
	
	<property name="seller" column="SELLER_ID" type="integer" />
	<property name="product" column="PRODUCT_ID" type="integer" />
	<property name="quantity" column="QUANTITY" type="integer" />
	<property name="price" column="PRICE" type="float" />
	
	<bag name="orders">
		<key column="WAREHOUSE_PRODUCT_ID" />
		<one-to-many class="com.ideas2it.ecommerce.Order" />
	</class>	
	
</hibernate-mapping>
     